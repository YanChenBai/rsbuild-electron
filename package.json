{
  "name": "rsbuild-electron",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "main": "./out/main/index.js",
  "scripts": {
    "check": "biome check --write",
    "dev": "chcp 65001 && run-p dev:main dev:preload dev:renderer",
    "dev:main": "pnpm --filter main run dev",
    "dev:preload": "pnpm --filter preload run dev",
    "dev:renderer": "pnpm --filter renderer run dev",
    "dev:electron": "electron ./out/main/index.js",
    "build:pred": "run-p build:main build:preload build:renderer",
    "build:main": "pnpm --filter main run build",
    "build:preload": "pnpm --filter preload run build",
    "build:renderer": "pnpm --filter renderer run build",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "preview": "rsbuild preview",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "pnpm build:pred && electron-builder --win",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@byc/tipc": "^0.0.92",
    "@electron/rebuild": "^3.7.1",
    "@prisma/client": "6.5.0",
    "npm-run-all2": "^7.0.2",
    "sharp": "^0.33.5",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@antfu/ni": "^0.23.2",
    "@rsbuild/core": "^1.1.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8",
    "eslint": "^9.22.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "hmc-win32": "^1.4.92",
    "jiti": "^2.4.2",
    "lodash-es": "^4.17.21",
    "prisma": "^6.5.0",
    "rsbuild-electron-restart": "workspace:*",
    "typescript": "^5.7.2",
    "unocss": "66.1.0-beta.5"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32"
      ],
      "cpu": [
        "x64"
      ]
    }
  }
}
